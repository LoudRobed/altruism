@article{keller_selfish_1998,
	title = {Selfish genes: a green beard in the red fire ant},
	volume = {394},
	copyright = {© 1998 Nature Publishing Group},
	issn = {0028-0836},
	shorttitle = {Selfish genes},
	url = {http://www.nature.com/nature/journal/v394/n6693/abs/394573a0.html},
	doi = {10.1038/29064},
	abstract = {A 'green-beard' gene is defined as a gene that causes a phenotypic effect (such as the presence of a green beard or any other conspicuous feature), allows the bearer of this feature to recognize it in other individuals, and causes the bearer to behave differently towards other individuals depending on whether or not they possess the feature. Such genes have been proposed on theoretical grounds to be agents mediating both altruism and intragenomic conflicts,, but until now few, if any, of these genes have been identified,. Here we provide evidence of a green-beard gene in the red imported fire ant, Solenopsis invicta. In polygyne (multiple-queen) colonies, all egg-laying queens are Bb heterozygotes at the locus Gp-9 (ref. 6). Previous studies suggested that bb females die prematurely from intrinsic causes; we now show that {BB} queens initiating reproduction are killed by workers, and that it is primarily Bb rather than {BB} workers that are responsible for these executions. This implies that allele Gp-9 b is linked to a green-beard allele that preferentially induces workers bearing the allele to kill all queens that do not bear it. Workers appear to distinguish {BB} from Bb queens on the basis of a transferable odour cue.},
	language = {en},
	number = {6693},
	urldate = {2013-12-09},
	journal = {Nature},
	author = {Keller, Laurent and Ross, Kenneth G.},
	month = aug,
	year = {1998},
	pages = {573--575},
	file = {Full Text PDF:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/5NSQ899H/Keller and Ross - 1998 - Selfish genes a green beard in the red fire ant.pdf:application/pdf;Snapshot:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/33GXREQB/394573a0.html:text/html}
},

@article{layzer_altruism_1978,
	title = {Altruism and natural selection},
	volume = {1},
	issn = {0140-1750},
	url = {http://www.sciencedirect.com/science/article/pii/0140175078900283},
	doi = {10.1016/0140-1750(78)90028-3},
	abstract = {According to current evolutionary theory, the evolutionary fate of any biological character is determined by its inclusive fitness (as defined by Haldane, 1932, and Hamilton, 1964). This paper discusses the concept of fitness for ‘interactive’ traits in the context of two recent theoretical developments: a mathematical approach to population genetics that takes the frequencies and fitnesses of whole phenotypes (instead of individual genes) as primitive variables (Layzer, 1978a); and a new hypothesis concerning genetic variation (Layzer, 1978b), which implies that functionally equivalent traits are likely to arise simultaneously in many distinct lineages in a population. As a result, the fitness (as here defined) of an interactive trait may differ significantly from its inclusive fitness (in the Haldane-Hamilton sense). In particular, group selection need not be invoked to account for the evolution of behaviors that diminish inclusive fitness.},
	number = {3},
	urldate = {2013-12-07},
	journal = {Journal of Social and Biological Structures},
	author = {Layzer, David},
	month = jul,
	year = {1978},
	pages = {297--305},
	file = {ScienceDirect Full Text PDF:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/IX5DQ3PU/Layzer - 1978 - Altruism and natural selection.pdf:application/pdf;ScienceDirect Snapshot:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/TZ2UGFZD/0140175078900283.html:text/html}
},

