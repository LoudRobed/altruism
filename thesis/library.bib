@incollection{hales_evolving_2003,
	series = {Lecture Notes in Computer Science},
	title = {Evolving Specialisation, Altruism, and Group-Level Optimisation Using Tags},
	copyright = {©2003 Springer-Verlag Berlin Heidelberg},
	isbn = {978-3-540-00607-7, 978-3-540-36483-2},
	url = {http://link.springer.com/chapter/10.1007/3-540-36483-8_3},
	abstract = {We present a model that demonstrates the evolution of groups composed of cooperative individuals performing specialised functions. Specialisation and cooperation results from an evolutionary process in which selection and reproduction is based on individual fitness. Specialists come to help (through the donation of resources) their non-kin group members, optimising their behaviour as a team and producing a fitter group. The mechanism that promotes this benevolent, cooperative group behaviour is based on the concept of a “tag”. Tags1 are observable markings, cues or displays. Individuals can observe the tags of others and take alternative actions based on those observations (e.g. to altruistically2 help or not). We show that even random (or dumb) searching for appropriate partners produces significant levels of specialisation and cooperation. Additionally we demonstrate that non-random (or smart) searching dramatically increases the effect.},
	language = {en},
	number = {2581},
	urldate = {2013-12-12},
	booktitle = {Multi-Agent-Based Simulation {II}},
	publisher = {Springer Berlin Heidelberg},
	author = {Hales, David},
	editor = {Sichman, Jaime Simão and Bousquet, Françcois and Davidsson, Paul},
	month = jan,
	year = {2003},
	keywords = {Artificial Intelligence (incl. Robotics), Computer Appl. in Social and Behavioral Sciences, Computer Communication Networks, Information Systems Applications ({incl.Internet)}, Simulation and Modeling, User Interfaces and Human Computer Interaction},
	pages = {26--35},
	file = {Full Text PDF:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/9UJQT957/Hales - 2003 - Evolving Specialisation, Altruism, and Group-Level.pdf:application/pdf;Full Text PDF:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/D78KIMIN/Hales - 2003 - Evolving Specialisation, Altruism, and Group-Level.pdf:application/pdf;Snapshot:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/J5HFWSFT/3-540-36483-8_3.html:text/html;Snapshot:/home/andreas/.zotero/zotero/x37w5qgi.default/zotero/storage/JWMMBW5H/10.html:text/html}
